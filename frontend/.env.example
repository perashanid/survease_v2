# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api

# Environment
VITE_ENV=development

# Application Configuration
VITE_APP_NAME=Survey Platform
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PUBLIC_SURVEYS=true
VITE_ENABLE_SURVEY_IMPORT=true
VITE_ENABLE_DARK_MODE=true

# Pagination
VITE_DEFAULT_PAGE_SIZE=12
VITE_MAX_PAGE_SIZE=50

# Survey Configuration
VITE_MAX_QUESTIONS_PER_SURVEY=100
VITE_MAX_OPTIONS_PER_QUESTION=50

# UI Configuration
VITE_ENABLE_ANIMATIONS=true
VITE_ANIMATION_DURATION=300

# Debug
VITE_DEBUG_MODE=false
VITE_SHOW_DEV_TOOLS=true

# Optional: Analytics or tracking IDs
# VITE_ANALYTICS_ID=your-analytics-id
# VITE_GA_TRACKING_ID=UA-XXXXXXXXX-X
